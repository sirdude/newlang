#!/usr/bin/sweet
# http://www.bagley.org/~doug/shootout/

int main() {
	int n, i, j, last;
	int *x, *y;

	n = argv[1];

	if (n < 0) {
		n = 1;
	}
	last = n - 1;

	while (i < last) {
		x[i] = i + 1;
		y[i] = 0;
		i = i + 1;
	}
	i = 1;
	while (i < 1000) {
		j = last;
		while (j > 0) {
			y[j] = y[j] + x[j];
			j = j - 1;
		}
	}

	write(y[0] + " " + y[last] + "\n");
}
