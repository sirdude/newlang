#!/usr/bin/sweet
# The Computer Language Benchmarks Game
# http://shootout.alioth.debian.org/

void do_work(int x, int y) {
	int h, w;

	h=0;
	M= 2 / x;
	ba = 2^(N % 8 + 1) -1;
	bb = 2^(8 - N % 8);
	while (h < y) {
		w = 0;
		Ci = y * M - 1;
		b = 1;
		p = 0;
		while (w < x) {
			Cr = x * M - 1.5;
			Zr = Cr;
			Zi = Ci;
			Zrq = Cr * Cr;
			Ziq = Ci * Ci;
			b = b + 1;
			i = 1;
			while ((i <= 49) && !done) {
				Zi = Zr * Zi * 2 + Ci;
				Zr = Zrq - Ziq + Cr;
				Ziq = Zi * Zi;
				Zrq = Zr * Zr;
				if (Zrq + Ziq > 4.0 ) {
					b = b+ 1;
					done = 1;
				} else {
					i = i + 1;
				}
			}
			w++;
		}
		if ( b >= 256 ) {
			p = p + 1;
			buf[p] = 511 - b;
			b = 1;
		}
		h++;
	}
	if (b ~= 1) {
		p = p + 1;
		buf[p] = (ba - b) * bb;
	}
	write(buf);
}

int main() {
	w = ARGV[0];
	if (!w) {
		w= 60;
	}
	h = w;

	write "P4\n" + w + " " + h + "\n";

	do_work(h,w);

	return 1;
}

