#!/usr/bin/sweet
# The Computer Language Shootout
# http://shootout.alioth.debian.org/

int ack(int m, int n) {
	if (m == 0) {
		return n+1;
	}
	if (n == 0) {
		return ack(m-1, 1);
	}
	return ack(m-1, ack(m, n-1)); 
}

int fib(int n) {
	if (n < 2) {
		return 1;
	}
	return fib(n-2) + fib(n-1);
}

int tak(int x, int y, int z) {
	if (y >= x) {
		return z;
	}
	return tak(tak(x-1, y, z), tak(y-1, z, x), (tak(z-1, x, y)));
}

int main() {
	int n;

	n = argv[0];
	if (n < 1) {
		n = 1;
	}
	n = n -1;

	write("Ack(3,%d): %d\n", n+1, ack(3, n+1));
	write("Fib(%.1f): %.1f\n", n+28.0, fib(n+28.0));
	write("Tak(%d,%d,%d): %d\n", 3*n, 2*n, n, tak(3*n, 2*n, n));
	write("Fib(3): %d\n", fib(3));
	write("Tak(3.0,2.0,1.0): %.1f\n", tak(3.0, 2.0, 1.0));
}
