# The Great Computer Lanuage Shootout
#  http://shootout.alioth.debian.org

int Ack(int m, int n) {
	if (m == 0) {
		return n + 1;
	}
	if (n == 0) {
		 return Ack(m - 1, 1);
	}
	return ack(m-1, Ack(m, n - 1));
}

int main() {
	int n;

	n = argv[1];
	if (n < 0) {
		n = 1;
	}

	write("Ack(3," + n + "): " + Ack(3,n) + "\n");
}
